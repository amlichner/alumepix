<style>
.banner-content-6 .title {
    color: #ffffff;
    font-size: 42px;
    font-weight: 800;
      line-height: 1.2;
}
  .banner-content-6 p {
    color: #ffffff;
    font-size: 15px;
    letter-spacing: 1px;
    font-weight: 500;
    margin: 20px 00;
}
 .banner-content-6 .title {
   color:{{section.settings.head_color}};
}
   .banner-content-6 p {
  color:{{section.settings.content_color}};
}
  section.banner-area-6.bg_cover4 {
    background-position: center center;
    background-repeat: no-repeat;
    background-size: cover;
    min-height: 85vh;
}
  .text-align{
    text-align:{{ section.settings.text_alignment}};
  }
  .banner-content-6 {
    transform: translate(0, 50%);
    -ms-transform: translate(0, 50%);
}
  .more{
  padding:10px 30px;
    font-weight:800;
  }
   @media (min-width:1200px) {
       section.banner-area-6.bg_cover4 {
    background-size: contain;
    min-height: 30vw;
}
   }
 @media (min-width:750px) {
   .hero__text-content--mobile{
   display:none;
   }
 }
   @media (max-width:749px) {
     .small--hide{
   display:none;
   }
 }
</style>
{% if section.settings.image !=blank %}
<section class="banner-area-6 bg_cover4 {% if section.settings.hero_layout == 'fixed_width' %}container{% endif %} small--hide" style="background-image: url({{section.settings.image|img_url:'original'}});">
<div class="text-alignm">
  {% else %}
  <span class="placeholder-disp-l">
    {% capture current %}{% cycle 1, 2 %}{% endcapture %}
    {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
  </span>
  {% endif %}  
  {% if section.settings.image !=blank %}  

  {%- assign text_alignments = section.settings.text_alignment | split: ' ' -%}
  {%- assign text_horizontal_alignment = text_alignments.first -%}
  {% endif %} 
  <div class="text-align container small--hide ">
    <div class="row  "> 
      <div class="col-lg-6">
        <div class="banner-content-6  ">
          <h6 class="sub-title"style="color:{{section.settings.subhead_color}}">{{section.settings.subhead1}}  </h6>
          <h6 class="sub-title"style="color:{{section.settings.subhead_color}}">{{section.settings.subhead2}}</h6>
          <h3 class="title">{{section.settings.heading}}</h3>
          <p>{{section.settings.content}}</p>
          {%- if section.settings.btn_link !=blank -%}
          <a href="{{section.settings.btn_link}}" class="more btne button-second"
             style=" color:{{section.settings.btn_color}};background-color:{{section.settings.btn_bg_color}};">{{section.settings.btn_label}}</a>
          {% endif %}
        </div>
      </div>
    </div></div>
  </div>
</section>

<div class="hero__text-content hero__text-content--mobile  text-center ">
  {% if section.settings.image1 !=blank %}
  <img src="{{section.settings.image1|img_url:'original'}}">
  {% else %}
  <span class="placeholder-disp-s">
    {% capture current %}{% cycle 1, 2 %}{% endcapture %}
    {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
  </span>
  {% endif %}
  <div class="container">
    <div class="offer-txt">
      <h6 class="sub-title"style="display:inline;{{section.settings.subhead_color}}">{{section.settings.subhead1}}  </h6>
      <h6 class="sub-title"style="display:inline;{{section.settings.subhead_color}}">{{section.settings.subhead2}}</h6>
    </div>
    {%- unless section.settings.heading == blank -%}
    <h3 class="title mega-title hero__title hero__title--mobile">
      {{ section.settings.heading | escape }}
    </h3>
    {%- endunless -%}
    {%- unless section.settings.content == blank -%}
    <p class="mega-subtitle hero__subtitle hero__subtitle--mobile">
      {{ section.settings.content | escape }}
    </p>
    {%- endunless -%}

    {%- if section.settings.btn_link !=blank -%}
    <a href="{{ section.settings.btn_link }}" class="btne hero__btn hero__btn--mobile more"style=" color:{{section.settings.btn_color}};background-color:{{section.settings.btn_bg_color}};">
      {{ section.settings.btn_label | escape }}
    </a>
    {%- endif -%}
  </div>
</div>

